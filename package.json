{
  "name": "lit-apka",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf built && rm -rf dist && yarn rollup -c rollup.config.js",
    "closure": "java -jar node_modules/google-closure-compiler-java/compiler.jar --flagfile closure.conf",
    "tsickle": "ts-node --project tsconfig.tools.json tsickle.ts -- -p apps",
    "serve": "gulp -f tools/gulpfile.ts dev",
    "build-prod": "gulp -f tools/gulpfile.ts prod"
  },
  "dependencies": {
    "@lit/reactive-element": "file:../lit2/packages/reactive-element",
    "lit": "file:../lit2/packages/lit",
    "lit-element": "file:../lit2/packages/lit-element",
    "lit-html": "file:../lit2/packages/lit-html"
  },
  "devDependencies": {
    "@types/browser-sync": "^2.26.3",
    "@types/google-closure-compiler": "^0.0.19",
    "@types/gulp": "^4.0.9",
    "@types/rimraf": "^3.0.2",
    "browser-sync": "^2.27.7",
    "closure-calculate-chunks": "^3.0.2",
    "google-closure-compiler": "^20211107.0.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tsickle": "^0.43.0",
    "typescript": "^4.4.4"
  }
}
